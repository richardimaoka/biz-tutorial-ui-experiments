  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch server",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "trace": "verbose",
      "program": "${workspaceFolder}/",
      "args": ["server"]
    },
    {
      "name": "Launch test function",
      "type": "go",
      "request": "launch",
      "mode": "test",
      "trace": "verbose",
      "program": "${workspaceFolder}/preprocess/processing",
      "args": ["-test.run", "TestFile"]
    },
    {
      "name": "Launch file",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}",
      "args": ["rough", "docker-cmd-entrypoint"]
    }
  ]
}
