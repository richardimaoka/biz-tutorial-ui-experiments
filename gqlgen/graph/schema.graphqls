# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Command {
  command: String
}

type CommandOutput {
  output: String
}

union TerminalElement = Command | CommandOutput

type Terminal {
  elements: [TerminalElement]
}

type IDE {
  focusedFile: String
}

type Step {
  terminal: Terminal
  ide: IDE
}

type Query {
  tutorial(id: ID): [Step]
}
